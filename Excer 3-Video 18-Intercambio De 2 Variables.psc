//Ejercicio 3: Intercambio de 2 Variables
Proceso principal
	Definir a,b,aux Como Enteros;
	
	Escribir "Digite el valor de A:";
	Leer a;
	Escribir "Digite el valor de B:";
	Leer b;
	aux<- a;
	a<- b;
	b<-aux;
	Escribir "El nuevo valor de A es:",a;
	Escribir "El nuevo valor de B es:",b;
FinProceso
